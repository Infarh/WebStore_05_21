@model IEnumerable<string>
@{
    ViewBag.Title = "WebAPI";
}

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>value</th>
            </tr>
        </thead>
        <tbody>
            @{ var i = 0; }
            @foreach (var value in Model)
            {
                <tr>
                    <td>@(i++)</td>
                    <td>@value</td>
                </tr>
            }
        </tbody>
    </table>
</div>
